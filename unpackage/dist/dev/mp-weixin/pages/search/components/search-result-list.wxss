
.search-result-list-wrap{
	width: 94%;
	margin-left: 3%;
}
._ul{
	margin: 0;
	padding: 0;
	list-style: none;
}
.search-result-list-wrap ._ul{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: space-evenly;
	-webkit-justify-content: space-evenly;
	        justify-content: space-evenly;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
}
.search-result-list-wrap ._ul:after{
	content: "";
	width: 45%;
}
.search-result-list-wrap ._ul ._li {
	width: 48%;
	background: #fff;
	border-radius: 20rpx;
	font-size: 24rpx;
	border: 2rpx solid #F1F1F1;
	padding: 20rpx 20rpx;
	box-sizing: border-box;
	margin-top: 20rpx;
}
.search-result-img{
	width: 80%;
	margin-left: 10%;
}
.search-result-img ._img{
	width: 100%;
}
.search-result-text{
	font-size: 28rpx;
}
.search-result-prize{
	margin-top: 20rpx;
	text-decoration: line-through;
	color: #AAAAAA;
	margin-bottom: 10rpx;
}
.search-result-pf{
	color: #FF465C;
}

