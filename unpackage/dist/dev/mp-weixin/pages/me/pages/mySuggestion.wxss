
.mySuggestion-warp{
	background-color: #F1F1F1;
	padding-top: 10rpx;
	padding-bottom: 10rpx;
}
.mySuggestion-title{
	width: 94%;
	margin-left: 3%;
	font-size: 28rpx;
}
.mySuggestion{
	background: #FFFFFF;
	width: 94%;
	margin-left: 3%;
	border-radius: 10rpx;
	padding: 20rpx 0rpx;
}
.mySuggestion-warp textarea{
	font-size: 28rpx;
	padding: 10rpx 10px;
	box-sizing: border-box;
}
.mySuggestion-cnt{
	margin-top: 10rpx;
	width: 94%;
	margin-left: 3%;
	border: 1px solid #F1F1F1;
	border-radius: 10rpx;
}

